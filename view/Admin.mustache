{{> header}}

<div class="w3-sidebar w3-bar-block w3-light-grey" style="z-index: 0">
    <h3 class="w3-bar-item">Administrador</h3>
    <button class="w3-button w3-block w3-left-align" onclick="myAccFunc()">
        Contenido <i class="fa fa-caret-down"></i>
    </button>
    <div id="demoAcc" class="w3-hide w3-white w3-card">
        <a href="/admin/productos" class="w3-bar-item w3-button w3-hover-grey">Productos</a>
        <a href="/admin/ediciones" class="w3-bar-item w3-button w3-hover-grey">Ediciones</a>
        <a href="/admin/contenidos" class="w3-bar-item w3-button w3-hover-grey">Contenidos</a>
    </div>
    <a href="/admin/usuarios" class="w3-bar-item w3-button w3-hover-teal">Usuarios</a>
    <a href="/admin/reportes" class="w3-bar-item w3-button w3-hover-teal">Reportes</a>
    <a href="/infonete/cerrarSesion" class="w3-bar-item w3-button w3-padding-large w3-hide-small w3-center w3-text-white w3-black">Cerrar Sesi√≥n</a>
</div>

<!-- Page content -->
<div class="w3-content contentWrap" style="max-width:2000px; padding-left: 200px">

<div class="w3-container w3-content w3-center w3-padding-30" style="margin-bottom: 30px" id="band">

    {{#productos}}
        {{> AdminProductos}}
    {{/productos}}
    {{#ediciones}}
        {{> AdminEdiciones}}
    {{/ediciones}}
    {{#contenidos}}
        {{> AdminContenidos}}
    {{/contenidos}}
    {{#usuarios}}
        {{> AdminUsuarios}}
    {{/usuarios}}
    {{#formAltaUsuarios}}
        {{> formAltaUsuarios}}
    {{/formAltaUsuarios}}
    {{#reportes}}
        {{> AdminReportes}}
    {{/reportes}}

    {{#graficos}}
        {{> adminGraficos}}
    {{/graficos}}

    </div>
</div>

{{> footer}}
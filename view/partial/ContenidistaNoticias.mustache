<div class="contAddUser">
    <h1>Mis noticias</h1>
</div>
<table class='w3-table w3-striped'>
    <thead>
    <tr class="w3-dark-grey">
        <th>Id</th>
        <th>Titulo</th>
        <th>Estado</th>
        <th>Acciones</th>
    </tr>
    </thead>
    {{#listaNoticias}}
        <tr>
            <td>{{id}}</td>
            <td>{{titulo}}</td>
            <td>{{estado}}</td>
            <td>
            <a href="../contenido?id={{id}}" class="w3-button w3-black w3-round-large">Ver noticia</a>
            <a href="/contenidista/editarNoticia?id={{id}}" class="w3-button w3-black w3-round-large">Editar</a>
            </td>
        </tr>
    {{/listaNoticias}}
</table>